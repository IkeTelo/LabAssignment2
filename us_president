firstName = input("Enter a first name: ")   ## no indentation
foundFlag = False
infile = open("USPres.txt", 'r')
for line in infile:
    if line.startswith(firstName + ' ') :  ## decision structure block - repetition block
        print(line.rstrip() )
        foundFlag = True
if not foundFlag:
    print("No president had the first name", firstName + '.')
